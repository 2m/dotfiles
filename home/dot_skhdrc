# open terminal
cmd - return : /Applications/Alacritty.app/Contents/MacOS/alacritty -e tmux-attach-or-new
shift + cmd - return : /Applications/Alacritty.app/Contents/MacOS/alacritty -e fish

# copy gpg passphrase to clipboard
cmd - g : /Applications/Alacritty.app/Contents/MacOS/alacritty -e lpass show --sync=no --notes gpg/master --clip
